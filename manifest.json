{

  "manifest_version": 2,
  "name": "Pintest",
  "version": "1.0",

  "description": "Adds arrow navigation to Pinterest",

  "icons": {
    "48": "icons/border-48.png"
  },

  "permissions": [
    "storage"
  ],

  "content_scripts": [
    {
      "matches": ["*://*.pinterest.fr/**"],
      "js": ["pintest.js"]
    }
  ]

}
